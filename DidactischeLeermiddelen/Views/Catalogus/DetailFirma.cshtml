@model DidactischeLeermiddelen.ViewModels.FirmaViewModel
<div class="paginaFirma">
    <div class="detailFirma col-sm-12">

        <table>
            <tr><th>Details van firma @Html.DisplayFor(m => m.Naam):</th></tr>
            <tr>
                <td>Naam: @Html.DisplayFor(modelItem => Model.Naam)</td>
            </tr>
            <tr>
                <td>Email: @Html.DisplayFor(modelItem => Model.Email)</td>
            </tr>
            <tr>
                <td>Contactpersoon: @Html.DisplayFor(modelItem => Model.Contactpersoon)</td>
            </tr>
            <tr>
                <td>Website: <a href="https://@Model.Website" target="blank">@Html.DisplayFor(m => m.Website)</a></td>
            </tr>
            <tr>
                <td>Adres: @Html.DisplayFor(modelItem => Model.Adres)</td>
            </tr>
        </table>

    </div>
    <div>
        @using (Html.BeginForm("Index", "Catalogus"))
        {
            <button type="submit" class="btn btn-default btn-verlanglijst ">
                &laquo; Catalogus
            </button>
        }
    </div>

</div>